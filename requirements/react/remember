function GenericButton () {
    return (
    <div>
      <button type="button" class="button2">SIGN IN</button>
    </div>)
}

function Banner() {
  const title = "Ft_TRANSCENDENCE"
    return (<h1>{title}</h1>)
  }

ReactDOM.render(<div>  <Banner /> <GenericButton />  </div>, document.getElementById('root'))




